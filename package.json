{
  "name": "eslint-plugin-test-comments",
  "description": "Enforces BDD style comments in your JavaScript tests! (given/when/then)",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/index.js",
  "keywords": [
    "es6",
    "eslint",
    "eslint-plugin"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project .",
    "format": "prettier --write",
    "lint": "tslint --fix --project ./tsconfig.json",
    "prelint": "npm run format",
    "prepack": "npm install && npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/node": "16.11.0",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/experimental-utils": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "jest": "27.2.5",
    "prettier": "2.4.1",
    "ts-jest": "27.0.6",
    "typescript": "4.4.4"
  }
}
